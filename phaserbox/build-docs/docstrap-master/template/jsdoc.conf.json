{
	"tags"      : {
		"allowUnknownTags" : true
	},
	"plugins"   : ["plugins/markdown"],
	"templates" : {
		"cleverLinks"     : false,
		"monospaceLinks"  : false,
		"default"         : {
			"outputSourceFiles" : true
		},
		"systemName"      : "DocStrap",
		"footer"          : "",
		"copyright"       : "DocStrap Copyright Â© 2012-2013 The contributors to the JSDoc3 and DocStrap projects.",
		"navType"         : "vertical",
		"theme"           : "cerulean",
		"linenums"        : true,
		"collapseSymbols" : false,
		"inverseNav"      : true
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
    "opts": {
        "template": "./build-docs/docstrap-master/template/",
        "encoding": "utf8",
        "destination": "./api-docs/",
        "recurse": false,
        "private": false,
        "lenient": true
    }
}
